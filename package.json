{
  "name": "fod",
  "version": "1.0.0",
  "description": "fod cypress",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "pretest": "npm run clean:reports",
    "start": "npm start",
    "ci": "start-server-and-test start-server http://localhost:8080",
    "run": "cypress run",
    "open": "cypress open",
    "delete:reports": "rm cypress/results/* || true",
    "prereport": "npm run delete:reports",
    "report": "cypress run",
    "combine-reports": "mochawesome-merge --reportDir cypress/reports/mocha > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest",
    "after:tests": "npm run merge-report; npm run generate-report"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^1.3.1",
    "cypress": "^4.3.0",
    "cypress-capybara": "0.0.2",
    "cypress-dark": "^1.7.14",
    "cypress-failed-log": "^2.6.1",
    "cypress-file-upload": "^3.5.3",
    "cypress-multi-reporters": "^1.2.4",
    "cypress-plugin-retries": "^1.5.2",
    "cypress-sql-server": "*",
    "cypress-wait-until": "^1.6.1",
    "cypress-xpath": "^1.4.0",
    "ejs": "^2.7.4",
    "express": "4.17.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^2.1.0",
    "mochawesome-report-generator": "^4.1.0",
    "mssql": "5.1.0",
    "rimraf": "^2.6.3",
    "start-server-and-test": "^1.10.8"
  },
  "author": "Yirga",
  "license": "ISC",
  "dependencies": {
    "@applitools/eyes-cypress": "^3.9.11",
    "JSONStream": "^1.3.5",
    "cypress-downloadfile": "^1.1.5",
    "mssql": "^5.1.0",
    "mysql": "^2.18.1",
    "npm": "^6.14.4"
  }
}
